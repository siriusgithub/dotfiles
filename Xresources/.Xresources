  URxvt*scrollColor: #000000
  URxvt*urgentOnBell: true
  URxvt*scrollstyle: plain
  URxvt*thickness: 5
  URxvt*scrollBar: false
  URxvt*scrollBar_right: true
  URxvt*scrollBar_floating: true
  URxvt*saveLines: 2000
  URxvt*internalBorder: 0
  URxvt*perl-ext-common: default,matcher,keyboard-select,vtwheel,clipboard,resize-font
  URxvt.url-launcher: /usr/bin/firefox
  URxvt.matcher.button: 1
  URxvt.keysym.Shift-M-u: perl:matcher:last
  URxvt.keysym.M-u: perl:matcher:list
  URxvt.keysym.M-Escape: perl:keyboard-select:activate
  URxvt*perl-ext: default,url-select
  URxvt.matcher.rend.0: Uline fg6
  URxvt*iso14755: false
